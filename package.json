{
  "name": "corny-gro≈Ñ:-peak-black",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "tsc --noEmit",
    "test": "tsc -p tests/tsconfig.tests.json && node --test ./tmp/tests/tests/*.test.js",
    "test:coverage": "tsc -p tests/tsconfig.tests.json && mkdir -p coverage && /bin/bash -lc 'set -o pipefail; NODE_V8_COVERAGE=coverage/v8 node --test --experimental-test-coverage ./tmp/tests/tests/*.test.js | tee coverage/summary.txt'",
    "build": "vite build",
    "preview": "vite preview",
    "harness": "tsc -p scripts/tsconfig.harness.json && node ./tmp/harness/scripts/harness.js"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
